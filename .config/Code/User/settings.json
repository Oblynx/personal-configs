{
  "go.useLanguageServer": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 10000,
  "editor.fontSize": 13,
  "editor.wordWrapColumn": 92,
  "editor.letterSpacing": 0.2,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.suggest.localityBonus": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.maxVisibleSuggestions": 10,
  "editor.suggest.shareSuggestSelections": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.panel.defaultLocation": "right",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
  "terminal.external.linuxExec": "gnome-terminal",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 10000,
  "telemetry.enableTelemetry": false,
  "go.toolsGopath": "~/go/",
  "markdown.preview.fontSize": 13,
  "vim.camelCaseMotion.enable": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-j>": false,
    "<C-`>": false,
    "<C-k>": false
  },
  "debug.console.fontSize": 13,
  "window.menuBarVisibility": "toggle",
  "todo-tree.highlights.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "todo-tree.tree.showScanModeButton": false,
  "liveshare.allowGuestDebugControl": true,
  "liveshare.guestApprovalRequired": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateFormat": "YYYY/MM/DD hh:mm",
  "gitlens.defaultDateShortFormat": "YY/MM/DD:hh",
  "diffEditor.renderSideBySide": false,
  "julia.enableTelemetry": true,
  "julia.format.indent": 2,
  "julia.useCustomSysimage": true,
  "julia.enableCrashReporter": true,
  "explorer.confirmDelete": false,
  "vim.commandLineModeKeyBindings": [],
  "git.autofetch": true,
  "editor.codeActionsOnSave": null,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.execution.resultType": "both",
  "julia.NumThreads": 10,
  "vim.highlightedyank.enable": true,
  "vim.history": 100,
  "vim.hlsearch": true,
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "go.toolsManagement.autoUpdate": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "vs-kubernetes": {
    "vs-kubernetes.namespace": "",
    "vs-kubernetes.kubectl-path": "",
    "vs-kubernetes.helm-path": "",
    "vs-kubernetes.minikube-path": "",
    "vs-kubernetes.kubectlVersioning": "user-provided",
    "vs-kubernetes.outputFormat": "yaml",
    "vs-kubernetes.kubeconfig": "/home/oblivion/clusters/kosamara-gpu/config-fwd",
    "vs-kubernetes.knownKubeconfigs": [
      "/home/oblivion/clusters/kosamara-gpu/config-fwd"
    ],
    "vs-kubernetes.autoCleanupOnDebugTerminate": false,
    "vs-kubernetes.nodejs-autodetect-remote-root": true,
    "vs-kubernetes.nodejs-remote-root": "",
    "vs-kubernetes.nodejs-debug-port": 9229,
    "vs-kubernetes.local-tunnel-debug-provider": "",
    "checkForMinikubeUpgrade": true,
    "imageBuildTool": "Docker"
  },
  "vscode-kubernetes.kubectl-path": "kubectl",
  "remote.containers.logLevel": "trace",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "redhat.telemetry.enabled": true,
  "julia.symbolCacheDownload": true,
  "tabnine.experimentalAutoImports": true,
  "gitlab.showPipelineUpdateNotifications": true,
  "gitlab.instanceUrl": "https://gitlab.cern.ch",
  "julia.usePlotPane": true,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.venvFolders": ["~/.local/share/virtualenvs"],
  "settingsSync.ignoredSettings": ["-python.venvFolders"],
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.languageServer": "Pylance",
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "files.associations": {
    "*.hcl": "terraform"
  },
  "cSpell.userWords": ["cloudlib", "kosamara", "microservices", "sqlalchemy"],
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "workbench.startupEditor": "none",
  "testing.defaultGutterClickAction": "debug",
  "yaml.customTags": [
    "!Base64 scalar",
    "!Cidr scalar",
    "!And sequence",
    "!Equals sequence",
    "!If sequence",
    "!Not sequence",
    "!Or sequence",
    "!Condition scalar",
    "!FindInMap sequence",
    "!GetAtt sequence",
    "!GetAZs scalar",
    "!ImportValue scalar",
    "!Join sequence",
    "!Select sequence",
    "!Split sequence",
    "!Sub scalar",
    "!Transform mapping",
    "!Ref scalar",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Join",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Split"
  ],
  "git.confirmSync": false,
  "update.mode": "manual",
  "logViewer.chunkSizeKb": 128,
  "logViewer.logLevel": "debug",
  "logViewer.watch": [

    {
      "groupName": "Devcontainer Flink",
      "watches": [
        {
          "title": "Localstack",
          "pattern": "/workspace/flink/log/localstack.log",
        },
        {
          "title": "Client",
          "pattern": "/workspace/flink/log/*client*.log",
        },
        {
          "title": "Python",
          "pattern": "/workspace/flink/log/*python*.log",
        }
    }
  ]
}
