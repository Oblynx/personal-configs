{
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 10000,
  "editor.fontSize": 13,
  "editor.wordWrapColumn": 92,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingIndent": "deepIndent",
  "editor.indentSize": "tabSize",
  "editor.letterSpacing": 0.2,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.suggest.localityBonus": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.shareSuggestSelections": true,
  "editor.inlineSuggest.enabled": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "diffEditor.experimental.showMoves": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.panel.defaultLocation": "right",
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "zenMode.hideLineNumbers": false,
  "terminal.external.linuxExec": "gnome-terminal",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 10000,
  "markdown.preview.fontSize": 13,
  "vim.camelCaseMotion.enable": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.statusBarColorControl": true,
  "vim.textwidth": 92,
  "vim.history": 100,
  "vim.commandLineModeKeyBindings": [],
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-j>": false,
    "<C-`>": false,
    "<C-k>": false
  },
  "debug.console.fontSize": 13,
  "todo-tree.highlights.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "liveshare.allowGuestDebugControl": true,
  "liveshare.guestApprovalRequired": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateFormat": "YYYY/MM/DD hh:mm",
  "gitlens.defaultDateShortFormat": "YY/MM/DD:hh",
  "diffEditor.renderSideBySide": false,
  "julia.enableTelemetry": true,
  "julia.useCustomSysimage": true,
  "julia.enableCrashReporter": true,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.execution.resultType": "both",
  "julia.NumThreads": 10,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "go.useLanguageServer": true,
  "go.toolsManagement.autoUpdate": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "dev.containers.logLevel": "debug",
  "gitlab.showPipelineUpdateNotifications": true,
  "julia.symbolCacheDownload": true,
  "julia.usePlotPane": true,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.venvFolders": ["~/.local/share/virtualenvs"],
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.languageServer": "Pylance",
  "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.formatOnSave": true,
  },
  "python.analysis.inlayHints.functionReturnTypes": true,
  "isort.args": [
      "--profile",
      "black"
  ],
  "editor.codeActionsOnSave": {

      "source.organizeImports": "always",
  },
  "settingsSync.ignoredSettings": ["-python.venvFolders"],
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "files.associations": {
    "*.hcl": "terraform"
  },
  "cSpell.userWords": ["cloudlib", "kosamara", "microservices", "sqlalchemy"],
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "jupyter.askForKernelRestart": false,
  "jupyter.allowUnauthorizedRemoteConnection": true,
  "jupyter.jupyterCommandLineArguments": [
      //"--NotebookApp.allow_origin=*",
  ],
  "jupyter.logKernelOutputSeparately": true,
  "jupyter.stopOnFirstLineWhileDebugging": false,
  "jupyter.themeMatplotlibPlots": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "testing.defaultGutterClickAction": "debug",
  "yaml.customTags": [
    "!Base64 scalar",
    "!Cidr scalar",
    "!And sequence",
    "!Equals sequence",
    "!If sequence",
    "!Not sequence",
    "!Or sequence",
    "!Condition scalar",
    "!FindInMap sequence",
    "!GetAtt sequence",
    "!GetAZs scalar",
    "!ImportValue scalar",
    "!Join sequence",
    "!Select sequence",
    "!Split sequence",
    "!Sub scalar",
    "!Transform mapping",
    "!Ref scalar",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Join",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Split"
  ],
  "git.confirmSync": false,
  "update.mode": "manual",
  "github.copilot.enable": {
      "*": true,
      "yaml": true,
      "plaintext": false,
      "markdown": false
  },
  "files.exclude": {
      "**/__pycache__": true,
      "**/.git": false,
  },
  "search.exclude": {
      ".git/*": true
  },
  "files.watcherExclude": {
      "**/.git": true,
  },
}